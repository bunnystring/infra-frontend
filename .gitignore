# Angular specific .gitignore

# Compiled output
/dist
/tmp
/out-tsc

# Runtime data
/*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
/package-lock.json
/pnpm-lock.yaml
/yarn.lock

# Directory for instrumented libs generated by jscoverage/JSCover
/lib-cov

# Coverage directory used by tools like istanbul
/coverage
/.nyc_output

# Dependency directories
/node_modules

# Compiled CSS
*.css
*.sass.css

# System Files
.DS_Store
Thumbs.db

# IDEs and editors
/.idea
/.vscode
*.sublime-workspace
*.sublime-project

# Environment files
.env
.env.*.local

# Angular environment files
/src/environments/environment*.ts

# Misc
/.angular/cache
/.angular/tsconfig.*

# Build artifacts
/build
